print('Zmienne typu string')
a=input('Wprowadz pierwsze zdanie:')
b=input('Wprowadz drugie zdanie:')
print(type(a))
print(a)
print(b)
#typ Int
c=input('Wprowadz liczbe calkowita:')
d=input('Wprowadz kolejna liczbe calkowita:')
c=int(c)
d=int(d)
print(c)
print(d)
print(type(c))
#Typ  float
e=input('Wprowadz liczbe zmiennoprzecinkowa:')
f=input('Podaj kolejna liczbe zmiennoprzecinkowa:')
e=float(e)
f=float(f)
print(e)
print(f)
print(type(e))
#Typ complex
g=input('Wprowadz pierwsze liczbe:')
h=input('Wprowadz druga liczbe:')
g=complex(g)
h=complex(h)
print(g)
print(h)
print(type(g))
